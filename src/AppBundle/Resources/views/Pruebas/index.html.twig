{% extends '::layout.html.twig' %}
{% block container %}
	{{ texto }}
	<hr>
	{{ fruta.manzana }}
	<hr>
	{% set variable = "Hola Twig" %}
	{{ variable }}
	<hr>
	{% if productos|length > 0 %}
		<ul>
			{% for producto in productos %}
				<li>{{ producto.producto }} - {{ producto.precio }}</li>
			{% endfor %}
		</ul>
	{% endif %}
	<hr>
	{% set fecha = date("-2 days", "Europe/Madrid") %}
	{{ dump(fecha) }}
	<hr>
	{{ include('AppBundle::partial.html.twig') }}
	<hr>
	{{max([76,99,58])}}
	<hr>
	{{min([76,99,58])}}
	<hr>
	{{random(100)}}
	<hr>
	{{generateTable(5,5)|raw}}
	<hr>
	{{ "Cadena ejemplo "|addText }}
{% endblock %}
